{
    "useTabs": false,
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSameLine": false,
    "parser": "flow",
    "semi": true,
    "overrides": [
        {
            "files": "*.tsx",
            "options": {
                "parser":"typescript"
            }
        },
        {
            "files": "*.ts",
            "options": {
                "parser":"typescript"
            }
        }
    ]
}